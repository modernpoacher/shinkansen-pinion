{
  "name": "shinkansen-pinion",
  "version": "1.1.515",
  "description": "Shinkansen Pinion",
  "keywords": [
    "Shinkansen",
    "Pinion",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri",
    "React"
  ],
  "main": "./src/index.cjs",
  "type": "module",
  "types": "./src/index.d.mts",
  "author": {
    "name": "Jonathan Perry for Modern Poacher Limited",
    "email": "modernpoacher@modernpoacher.com",
    "url": "https://modernpoacher.com"
  },
  "license": "ISC",
  "engines": {
    "node": ">=18.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/modernpoacher/shinkansen-pinion.git"
  },
  "scripts": {
    "build": "cross-env DEBUG=shinkansen-pinion* webpack",
    "build-storybook": "storybook build",
    "build:watch": "cross-env DEBUG=shinkansen-pinion* webpack --watch",
    "lint": "eslint . .storybook --ext .mjs,.cjs,.jsx,.mts,.cts",
    "lint:fix": "npm run lint -- --fix",
    "nvm": "bash nvm.sh",
    "prelint": "npm run nvm",
    "prepare": "husky",
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "prestorybook": "npm run build && npm run transform",
    "pretest": "npm run nvm",
    "storybook": "storybook dev -p 6009",
    "test": "npm run test:mocha && npm run test:jest",
    "test:jest": "cross-env NODE_ENV=test jest",
    "test:mocha": "cross-env NODE_ENV=test mocha test --recursive",
    "transform": "cross-env DEBUG=shinkansen-pinion* ./transform.sh"
  },
  "dependencies": {
    "debug": "^4.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/register": "^7.25.9",
    "@modernpoacher/design-system": "1.1.17",
    "@storybook/addon-actions": "7.6.20",
    "@storybook/addon-essentials": "7.6.20",
    "@storybook/addon-links": "7.6.20",
    "@storybook/react": "7.6.20",
    "@storybook/react-webpack5": "7.6.20",
    "@types/react": "^18.3.12",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "autoprefixer": "^10.4.20",
    "chai": "^5.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.39.0",
    "cross-env": "^7.0.3",
    "eslint": "8.57.1",
    "eslint-config-love": "47.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-react": "^7.37.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "mocha": "^10.8.2",
    "perfectionist": "^2.4.0",
    "postcss": "^8.4.49",
    "postcss-discard-comments": "^7.0.3",
    "postcss-discard-empty": "^7.0.0",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-map": "^0.11.0",
    "postcss-normalize": "^13.0.1",
    "postcss-scss": "^4.0.9",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.0.1",
    "react-router-dom": "^7.0.1",
    "react-test-renderer": "^18.3.1",
    "remove-files-webpack-plugin": "^1.5.0",
    "sass": "^1.81.0",
    "sass-loader": "^16.0.3",
    "shinkansen-cogs": "1.7.275",
    "shinkansen-rails": "1.0.255",
    "shinkansen-signals": "1.1.184",
    "shinkansen-sprockets": "1.1.433",
    "sinon": "^19.0.2",
    "storybook": "7.6.20",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "shinkansen-cogs": "1.7.x",
    "shinkansen-rails": "1.0.x",
    "shinkansen-signals": "1.1.x",
    "shinkansen-sprockets": "1.1.x"
  },
  "imports": {
    "#build/paths": "./build/paths.mjs",
    "#pinion": "./src/index.mjs",
    "#pinion/common": "./src/common/index.mjs",
    "#pinion/components": "./src/components/index.mjs",
    "#pinion/components/*": "./src/components/*/index.mjs",
    "#pinion/pinion": "./src/pinion/index.jsx",
    "#pinion/pinion/*": "./src/pinion/*/index.jsx",
    "#pinion/transformers": "./src/transformers/index.mjs",
    "#pinion/transformers/*": "./src/transformers/*/index.mjs",
    "#pinion/transformers/check-answers/*": "./src/transformers/check-answers/*.mjs",
    "#pinion/transformers/common/*": "./src/transformers/common/*.mjs",
    "#pinion/transformers/error-message/*": "./src/transformers/error-message/*.mjs",
    "#stories/components/*": "./stories/components/*/index.stories.jsx",
    "#stories/definitions/*": "./stories/definitions/*.json",
    "#stories/errors": "./stories/errors.mjs",
    "#stories/pinion/*": "./stories/pinion/*/index.stories.jsx",
    "#stories/state": "./stories/state/index.jsx",
    "#stories/state/*": "./stories/state/*/index.jsx"
  },
  "exports": {
    ".": {
      "require": "./src/index.cjs",
      "import": "./src/index.mjs"
    },
    "./common": "./src/common/index.mjs",
    "./components": {
      "require": "./src/components/index.cjs",
      "import": "./src/components/index.mjs"
    },
    "./components/*": {
      "require": "./src/components/*/index.cjs",
      "import": "./src/components/*/index.mjs"
    },
    "./pinion": "./src/pinion/index.cjs",
    "./pinion/*": "./src/pinion/*/index.cjs",
    "./transformers": "./src/transformers/index.mjs",
    "./transformers/check-answers": "./src/transformers/check-answers/index.mjs",
    "./transformers/check-answers/*": "./src/transformers/check-answers/*.mjs",
    "./transformers/common": "./src/transformers/common/index.mjs",
    "./transformers/common/*": "./src/transformers/common/*.mjs",
    "./transformers/error-message": "./src/transformers/error-message/index.mjs",
    "./transformers/error-message/*": "./src/transformers/error-message/*.mjs"
  }
}
