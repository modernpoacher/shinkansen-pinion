{
  "name": "shinkansen-pinion",
  "version": "1.0.43",
  "description": "Shinkansen Pinion",
  "keywords": [
    "Shinkansen",
    "Pinion",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri",
    "React"
  ],
  "main": "./src/index.cjs",
  "type": "module",
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/shinkansen-pinion.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "prebuild": "npm run clean",
    "build": "cross-env DEBUG=shinkansen-pinion npm run gulp -- build",
    "build:watch": "cross-env DEBUG=shinkansen-pinion npm run gulp -- build:watch",
    "clean": "rimraf lib",
    "transform": "cross-env DEBUG=shinkansen-pinion npm run gulp -- transform",
    "transform:watch": "cross-env DEBUG=shinkansen-pinion npm run gulp -- transform:watch",
    "pregulp": "npm run nvm",
    "gulp": "gulp --color",
    "prelint": "npm run nvm",
    "lint": "eslint . .storybook --ext .mjs --ext .cjs",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run nvm",
    "test": "cross-env NODE_ENV=test jest --collect-coverage false --verbose --run-in-band",
    "storybook": "npm run build && npm run transform && start-storybook -p 6009",
    "nvm": "bash nvm.sh"
  },
  "dependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-syntax-jsx": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.21.0",
    "@modernpoacher/design-system": "1.0.2",
    "@modernpoacher/hooks": "^1.0.394",
    "@sequencemedia/gulp": "^1.0.6",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/react": "6.5.12",
    "autoprefixer": "^10.4.14",
    "babel-plugin-module-resolver": "^5.0.0",
    "core-js": "^3.30.1",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "del": "^7.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "^8.39.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "glob-all": "^3.3.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-css-purge": "^3.0.9",
    "gulp-debug": "^4.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "postcss": "^8.4.23",
    "postcss-easy-import": "^4.0.0",
    "postcss-map": "^0.11.0",
    "postcss-normalize": "^10.0.1",
    "postcss-scss": "^4.0.6",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-test-renderer": "16.14.0",
    "regenerator-runtime": "^0.13.11",
    "rimraf": "^5.0.0",
    "sass": "^1.62.1",
    "shinkansen-cogs": "^1.5.48",
    "shinkansen-rails": "^1.0.23",
    "shinkansen-signals": "^1.1.8",
    "shinkansen-sprockets": "^1.0.50",
    "vinyl-paths": "3.0.1"
  },
  "imports": {
    "#build/gulp/build/*": "./build/gulp/build/*.mjs",
    "#build/gulp/build": "./build/gulp/build/index.mjs",
    "#build/gulp/paths": "./build/gulp/paths.mjs",
    "#build/gulp/handle-watch-error": "./build/gulp/handle-watch-error.mjs",
    "#build/gulp/transform": "./build/gulp/transform/index.mjs",
    "#build/paths": "./build/paths.mjs"
  },
  "exports": {
    ".": {
      "require": "./src/index.cjs",
      "import": "./src/index.mjs"
    },
    "./components/check-answers": "./src/components/check-answers/index.cjs",
    "./components/common/text-content": "./src/components/common/text-content/index.cjs",
    "./components/common": "./src/components/common/index.mjs",
    "./components/error-summary": "./src/components/error-summary/index.mjs",
    "./components/pinion": "./src/components/pinion/index.cjs",
    "./components": "./src/components/index.mjs",
    "./transformers/check-answers/*": "./src/transformers/check-answers/*.mjs",
    "./transformers/check-answers": "./src/transformers/check-answers/index.mjs",
    "./transformers/common/*": "./src/transformers/common/*.mjs",
    "./transformers/common": "./src/transformers/common/index.mjs",
    "./transformers": "./src/transformers/index.mjs",
    "./transformers/error-message": "./src/transformers/error-message/index.mjs"
  }
}
